<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 자주 사용하는 type 의 별칭 지정하기 -->
	<typeAliases>
		<typeAlias type="com.dowon.project.member.dto.MemberDto" alias="memberDto"/>
		<typeAlias type="com.dowon.project.cafe.dto.CafeDto" alias="cafeDto"/>
		<typeAlias type="com.dowon.project.users.dto.UsersDto" alias="usersDto"/>
	    <typeAlias type="com.dowon.project.file.dto.FileDto" alias="fileDto"/>
	</typeAliases>

	<!-- mapper 설정 -->
	<mappers>
		<mapper resource="com/dowon/project/mybatis/MemberMapper.xml"/>
		<mapper resource="com/dowon/project/mybatis/CafeMapper.xml"/>
		<mapper resource="com/dowon/project/mybatis/UsersMapper.xml"/>
		<mapper resource="com/dowon/project/mybatis/FileMapper.xml"/>
	</mappers>
</configuration>




